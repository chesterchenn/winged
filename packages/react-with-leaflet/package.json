{
  "name": "template-webpack",
  "sideEffects": false,
  "version": "1.0.1",
  "description": "quick react webpack template",
  "private": true,
  "scripts": {
    "start": "ts-node bin/start.ts",
    "clear": "ts-node bin/clear.ts",
    "init": "ts-node bin/init.ts",
    "init:ts": "ts-node bin/init.ts --type typescript",
    "init:js": "ts-node bin/init.ts --type javascript",
    "build": "webpack --config webpack/webpack.prod.ts"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "11.0.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "leaflet": "^1.9.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@types/cross-spawn": "^6.0.2",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-env": "^1.18.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "html-webpack-plugin": "5.5.3",
    "sass": "^1.64.2",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "thread-loader": "^4.0.2",
    "ts-loader": "9.4.4",
    "webpack": "5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  },
  "keywords": [
    "template",
    "react",
    "webpack"
  ],
  "author": "chesterchenn",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
